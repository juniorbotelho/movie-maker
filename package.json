{
  "name": "videomaker",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:juniorbotelho/videomaker.git",
  "author": "Junior Botelho <junowerther@gmail.com>",
  "description": "",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "cli": "cli/bin/cli"
  },
  "_moduleAliases": {
    "@Type": "build/src/types",
    "@App": "build/app",
    "@Cli": "build/cli",
    "@Config": "build/config",
    "@Service": "build/service",
    "@Temp": "build/temp",
    "@Source": "build/src",
    "@Utilities": "build/utilities"
  },
  "scripts": {
    "start": "node --require module-alias/register build/src/index.js",
    "dev:sucrase": "nodemon --watch '**/*' --require tsconfig-paths/register --config",
    "dev:typescript": "nodemon --watch '**/*' --require tsconfig-paths/register --config",
    "dev:videomaker": "node --require tsconfig-paths/register cli/bin/cli",
    "prebuild": "rimraf build/*",
    "build": "tsc -p tsconfig.json"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "algorithmia": "^0.3.10",
    "gluegun": "^4.6.1",
    "googleapis": "^87.0.0",
    "ibm-watson": "^6.2.1",
    "module-alias": "2.2.2",
    "sbd": "^1.0.19"
  },
  "devDependencies": {
    "@sentry/types": "^6.13.2",
    "@types/node": "12.7.11",
    "@types/sbd": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv": "10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "3.0.2",
    "sucrase": "^3.20.1",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
