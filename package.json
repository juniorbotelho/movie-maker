{
  "name": "videomaker",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:juniorbotelho/videomaker.git",
  "author": "Junior Botelho <junowerther@gmail.com>",
  "description": "",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "cli": "cli/bin/cli"
  },
  "_moduleAliases": {
    "@Type": "build/src/types",
    "@App": "build/app",
    "@Cli": "build/cli",
    "@Container": "build/container",
    "@Config": "build/config",
    "@Database": "build/database",
    "@Service": "build/service",
    "@Temp": "build/temp",
    "@Source": "build/src",
    "@Utilities": "build/utilities",
    "@Sites": "build/www"
  },
  "scripts": {
    "start:server": "node --require module-alias/register build/src/index.js",
    "start:cli": "node --require module-alias/register build/cli/cli.js",
    "dev:sucrase": "nodemon --watch '**/*' --require tsconfig-paths/register --config",
    "dev:typescript": "nodemon --watch '**/*' --require tsconfig-paths/register --config",
    "dev:cli": "node --require tsconfig-paths/register cli/bin/cli",
    "prebuild": "rimraf build/*",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r temp ./build"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "algorithmia": "^0.3.10",
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.10",
    "deepai": "^1.0.17",
    "express": "^4.17.1",
    "gluegun": "^4.6.1",
    "googleapis": "^87.0.0",
    "ibm-watson": "^6.2.1",
    "lexrank": "^1.0.5",
    "module-alias": "2.2.2",
    "node-html-parser": "^4.1.5",
    "pptxgenjs": "^3.8.0",
    "prompts": "^2.4.1",
    "sanitize-html": "^2.5.1",
    "sbd": "^1.0.19",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "videoshow": "^0.1.12"
  },
  "devDependencies": {
    "@sentry/types": "^6.13.2",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/node": "12.7.11",
    "@types/prompt": "^1.1.1",
    "@types/sanitize-html": "^2.3.2",
    "@types/sbd": "^1.0.2",
    "@types/sequelize": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv": "10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "3.0.2",
    "sucrase": "^3.20.1",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
