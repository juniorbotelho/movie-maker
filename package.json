{
  "name": "videomaker",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:juniorbotelho/videomaker.git",
  "author": "Junior Botelho <junowerther@gmail.com>",
  "description": "",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "cli": "bin/cli"
  },
  "_moduleAliases": {
    "app": "build/app",
    "config": "build/config",
    "server": "build/server",
    "providers": "build/providers"
  },
  "scripts": {
    "start": "node -r 'module-alias/register' build/index.js",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r 'tsconfig-paths/register' src/index.ts' -e ts",
    "prebuild": "rimraf build/*",
    "build": "tsc -p tsconfig.json"
  },
  "license": "MIT",
  "dependencies": {
    "gluegun": "4.6.1",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "@types/node": "12.7.11",
    "dotenv": "10.0.0",
    "nodemon": "2.0.12",
    "rimraf": "3.0.2",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
